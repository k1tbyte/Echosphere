{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "${DB_CONNECTION_STRING}"
  },
  "JwtSettings": {
    "Issuer": "EchosphereAuth",
    "Key": "${JWT_SIGN_KEY}"
  },
  "Minio": {
    "Endpoint": "${S3_SERVER_URL}",
    "Username": "${S3_SERVER_USERNAME}",
    "Password": "${S3_SERVER_PASSWORD}",
    "WithSSL": false
  },
  "Frontend": {
    "URL": "${FRONTEND_URL}"
  },
  "ffmpeg": 
  {
    "Path": "${FFMPEG_ROOT_PATH}"
  }
}
